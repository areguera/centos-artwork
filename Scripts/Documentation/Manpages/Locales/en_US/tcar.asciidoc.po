"<emphasis role=\"strong\">tcar</emphasis> [<emphasis role=\"strong"
"\">--help</emphasis>|<emphasis role=\"strong\">--version</emphasis>]"
msgstr ""
"<emphasis role=\"strong\">tcar</emphasis> [<emphasis role=\"strong"
"\">--help</emphasis>|<emphasis role=\"strong\">--version</emphasis>]"

